

//Create array of 1 to n

//push=insert at end
//unshift= insert at start
//pop=remove at end
//shift=remove at start
//splice=remove or add element
//concat




const strings=['Hi','Hello','World','X','Y','Z']
for(let i=0;i<strings.length;i++)
{
    if(strings[i]=='X')
    {
        strings.splice(i,1);
        break;
    }
}
console.log(strings);




let str='(a+b)+(c';

let len = str.length;
let count1 = 0, count2 = 0;

for (let i = 0; i < len; i++)
{
    if(str[i] == '(')
    count1++;
    else if(str[i] == ')')
    count2++;
    
}
console.log(count1);
console.log(count2);

if(count2 == count1)
{
    console.log('True');
}
else
{
console.log('False');
}


output: 
2
1
False


//is number square

function isSquare(num) 
{
    for(let i=1;i<=num;i++)
    {
        console.log('checking for',i);
        let sq=i*i;
        if(sq==num)
        {
            console.log('Got a match')
            return true;
        }
    }
    console.log('No match Found')
    return false;
}

console.log(isSquare(4));

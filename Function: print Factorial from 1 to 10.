


//print Factorial from 1 to 10.

function factorial(n) 
{
let out=1;
    for(let i=1;i<=n;i++)
    {
    out=out*i;
    }
    return out;    
}

function printFactorial(num)
{
    for(let i=1;i<=num;i++)
    {
    let fact=factorial(i);
    console.log('Factorial of',i,'is',fact);
    }
}

printFactorial(5);

output:
Factorial of 1 is 1
Factorial of 2 is 2
Factorial of 3 is 6
Factorial of 4 is 24
Factorial of 5 is 120

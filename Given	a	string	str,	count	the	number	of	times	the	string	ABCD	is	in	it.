/*Given	a	string	str,	count	the	number	of	times	the	string	ABCD	is	in	it.*/




var str = "AB	ABC	ABCD	ABCD	ABC	AB";
let m=str.length;
let count=0;
for(i=0;i<m;i++)
 {
    if (str[i]=='A' && str[i+1]=='B' && str[i+2]=='C' && str[i+3]=='D')
    {
        count++;
    }
 }
 console.log('Count of ABCD ='+count);

OUTPUT:Count of ABCD =2

/*Given an array as input, print ALLSAME if all the numbers in the array are the same otherwise print DIFFERENT.*/


 function findCom(arr)
{
    let len = arr.length;
   for(var i=0;i<len;i++)
   {
    if(arr[0]!=arr[i])
       return console.log('DIFFERENT');
   }
   return console.log('ALLSAME');
}

const a=[12,12,12,12];
   findCom(a);
   
   O/P= ALLSAME




 function EvenNum(arr) {
    let newArray = [];

    for (var i = 0; i < arr.length; i++) {
        if (arr[i] % 2 === 0) {
           newArray.unshift(arr[i]);
        }
    }
    return newArray;
}
var arr = [5,18,26,30,40,6,3];
console.log(EvenNum(arr));    
    
   
    O/P= [ 6, 40, 30, 26, 18 ]





function CountGreater(arr)
{
    let count = 0;
    let value =[ ];
    for (let i = 0; i < arr.length; i++)
    {
     
        if (arr[i]>21)
        {
           value[count++] = arr[i];
        }  
                  
    }
 
   console.log("Count = " + count);
    console.log("Values = " + value);
}
const arr =[100,200,10,20,50,64];

    CountGreater(arr);
    
    
 output:
    Count = 4
    Values = 100,200,50,64
    

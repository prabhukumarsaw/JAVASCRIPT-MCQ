    name: 'Mary',
    dept: 'Finance',
    Designation: 'Trainee',
    salary: '18500',
    raise: true
}
let emp3 = {
    name: 'Bill',
    dept: 'HR',
    Designation: 'Executive',
    salary: '212000',
    raise: true
}


// companyName, website, employees
// Tech Stars, www.techstars.site, array of employees

